# RewriteCond %{HTTP_HOST} ^www\.mrbet365\.us$
# RewriteRule ^/?$ "https\:\/\/www\.mrbet365\.us\/" [R=301,L]
AddType application/x-httpd-php .php
AddType application/x-httpd-php .php
AddType application/x-httpd-php .php
AddType application/x-httpd-php php_fileinfo

# php -- BEGIN cPanel-generated handler, do not edit
